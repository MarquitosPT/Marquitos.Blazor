@if (ChildContent != null || !string.IsNullOrWhiteSpace(Content.Value))
{
<svg class="mq-icon" viewBox="@ViewBox" @oncontextmenu="(e) => { }" @oncontextmenu:preventDefault="true" @oncontextmenu:stopPropagation="true">
    @if (ChildContent != null)
    {
        @ChildContent
    }
    else
    {
        @Content
    }
</svg>
}
